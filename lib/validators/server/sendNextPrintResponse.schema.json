{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://apexslicer.com/schemas/cloud/sendNextPrintResponse.json",
  "title": "sendNextPrintResponse",
  "description": "Printer server response to a sendNextPrint command",
  "type": "object",
  "properties": {
    "serialNumber": {
      "$ref": "../defs.json#/definitions/mongoObjectId"
    },
    "status": {
      "description": "Response status to failed sendNextPrint command",
      "type": "string",
      "enum": [
        "NONE",
        "FAILED"
      ]
    },
    "message": {
      "description": "Explanation of why no next print response",
      "type": "string",
      "enum": [
        "INVALID_JOBID",
        "NO_QUEUED_JOBS",
        "PRINTER_DELETED",
        "SERVER_ERROR"
      ]
    }
  },
  "oneOf": [
    {
      "properties": {
        "status": {
          "enum": [
            "NONE"
          ]
        },
        "message": {
          "enum": [
            "NO_QUEUED_JOBS"
          ]
        }
      },
      "required": [
        "serialNumber",
        "status",
        "message"
      ]
    },
    {
      "properties": {
        "status": {
          "enum": [
            "FAILED"
          ]
        },
        "message": {
          "enum": [
            "INVALID_JOBID",
            "PRINTER_DELETED",
            "SERVER_ERROR"
          ]
        }
      },
      "required": [
        "serialNumber",
        "status",
        "message"
      ]
    }
  ]
}
